<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox spacing="20" alignment="CENTER_LEFT" xmlns:fx="http://javafx.com/fxml/1" prefWidth="1200" fx:controller="ribera.practicapartes.Controllers.BarraNavegacion">
    <ImageView fitHeight="30" fitWidth="30">
        <javafx.scene.image.Image url="@/images/logo_icono.jpg"/>
    </ImageView>

    <Button text="Crear Parte" style="-fx-text-fill: #333;" fx:id="btnCrearParte">
        <font>
            <Font name="Arial" size="14"/>
        </font>
    </Button>
    <Button text="Lista de Partes" style="-fx-text-fill: #333;" fx:id="btnListaPartes">
        <font>
            <Font name="Arial" size="14"/>
        </font>
    </Button>
    <Button text="Lista de Alumnos" style="-fx-text-fill: #333;" fx:id="btnListaAlumnos">
        <font>
            <Font name="Arial" size="14"/>
        </font>
    </Button>
    <Button text="Crear Profesor" style="-fx-text-fill: #333;" fx:id="btnCrearProfesor">
        <font>
            <Font name="Arial" size="14"/>
        </font>
    </Button>
</HBox>
